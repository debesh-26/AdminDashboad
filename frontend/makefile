# Makefile for Inventory Management Dashboard

# Install project dependencies
install:
	npm install

# Start the development server
start:
	npm start

# Build the project for production
build:
	npm run build

# Run tests
test:
	npm test

# Clean the build directory
clean:
	rm -rf build

# Lint the project
lint:
	npm run lint

# Format the code
format:
	npm run format
